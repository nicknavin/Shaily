<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

 <include layout="@layout/toolbar_framethumb"></include>

    <com.app.session.thumby.CenterCropVideoView
            android:id="@+id/view_thumbnail"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"

            android:layout_marginBottom="64dp"



        />

    <com.app.session.thumby.ThumbnailTimeline
            android:id="@+id/thumbs"
            android:layout_width="wrap_content"
            android:layout_height="@dimen/frames_video_height"
            android:layout_gravity="center_horizontal" />

</LinearLayout>